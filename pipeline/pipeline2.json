{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbAtlasSource",
						"batchSize": 100
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['_id']['$oid']"
								},
								"sink": {
									"name": "$oid"
								}
							},
							{
								"source": {
									"path": "$['fullName']"
								},
								"sink": {
									"name": "fullName"
								}
							},
							{
								"source": {
									"path": "$['email']"
								},
								"sink": {
									"name": "email"
								}
							},
							{
								"source": {
									"path": "$['password']"
								},
								"sink": {
									"name": "password"
								}
							},
							{
								"source": {
									"path": "$['role']"
								},
								"sink": {
									"name": "role"
								}
							},
							{
								"source": {
									"path": "$['createdAt']['$date']['$numberLong']"
								},
								"sink": {
									"name": "$numberLong"
								}
							},
							{
								"source": {
									"path": "$['__v']['$numberInt']"
								},
								"sink": {
									"name": "$numberInt"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_MONGO_Users",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SQL_Users",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}