{
	"name": "DF_CLEANED_TO_REPORT",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "projectsSqlCleaned"
				},
				{
					"name": "tasksSqlCleaned"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "projectsAndTasks"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_id} as string,",
				"          name as string,",
				"          description as string,",
				"          userId as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> projectsSqlCleaned",
				"source(output(",
				"          {_id} as string,",
				"          name as string,",
				"          description as string,",
				"          status as string,",
				"          userId as string,",
				"          projectId as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> tasksSqlCleaned",
				"projectsSqlCleaned, tasksSqlCleaned join(projectsSqlCleaned@{_id} == projectId,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> projectsAndTasks"
			]
		}
	}
}