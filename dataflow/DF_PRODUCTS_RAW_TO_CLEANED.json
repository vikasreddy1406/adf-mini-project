{
	"name": "DF_PRODUCTS_RAW_TO_CLEANED",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL_PARAMETERS",
						"type": "DatasetReference"
					},
					"name": "productsRaw"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          departmentCode as string,",
				"          categoryCode as string,",
				"          categoryName as string,",
				"          searchCategoryCode as string,",
				"          seriesCode as string,",
				"          seriesName as string,",
				"          brandCode as string,",
				"          brandUrlCode as string,",
				"          brandName as string,",
				"          seriesStatus as string,",
				"          minDaysToShip as string,",
				"          maxDaysToShip as string,",
				"          minPrice as string,",
				"          maxPrice as string,",
				"          minPricePerPiece as string,",
				"          maxPricePerPiece as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> productsRaw",
				"productsRaw aggregate(groupBy(seriesName),",
				"     departmentCode = first(departmentCode),",
				"          categoryCode = first(categoryCode),",
				"          categoryName = first(categoryName),",
				"          searchCategoryCode = first(searchCategoryCode),",
				"          seriesCode = first(seriesCode),",
				"          brandCode = first(brandCode),",
				"          brandUrlCode = first(brandUrlCode),",
				"          brandName = first(brandName)) ~> aggregate1"
			]
		}
	}
}