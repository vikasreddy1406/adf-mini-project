{
	"name": "DF_RAW_TO_REPORT",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL_Users",
						"type": "DatasetReference"
					},
					"name": "userRawData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_id} as string,",
				"          fullName as string,",
				"          email as string,",
				"          role as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> userRawData",
				"derivedColumn1 filter(ERROR_FUNCTION('!isNull(_id) || !isNull(role)__NEW_LINE_R____NEW_LINE_N__')) ~> filter1",
				"userRawData derive({_id} = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}